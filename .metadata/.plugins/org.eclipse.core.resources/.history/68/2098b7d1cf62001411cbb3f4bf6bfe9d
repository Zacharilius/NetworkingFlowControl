package flowControl;


import java.io.*;
import java.net.*;
public class Server{
    public static void main(String args[]){
    	byte[] sendBytes = new byte[1024];
    	String s = "tada";
    	System.out.println("sendBytes.length: " + sendBytes.length);
    	sendBytes = s.getBytes();
    	System.out.println("sendBytes.length: " + sendBytes.length);

    }
}
